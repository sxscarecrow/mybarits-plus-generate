<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shenxu.royaltyV1.repostory.RateRepostory">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.shenxu.royaltyV1.domain.Rate">
        <id column="id" property="id" />
        <result column="currency_code_from" property="currencyCodeFrom" />
        <result column="currency_code_to" property="currencyCodeTo" />
        <result column="rate" property="rate" />
        <result column="month" property="month" />
        <result column="type" property="type" />
        <result column="created_time" property="createdTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, currency_code_from, currency_code_to, rate, month, type, created_time
    </sql>

</mapper>
